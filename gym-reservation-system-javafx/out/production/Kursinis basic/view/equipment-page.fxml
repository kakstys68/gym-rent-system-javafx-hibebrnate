<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="425.0" prefWidth="309.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.EquipmentPage">
   <children>
