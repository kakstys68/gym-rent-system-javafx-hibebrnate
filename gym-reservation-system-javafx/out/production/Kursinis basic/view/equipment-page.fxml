         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Text>
      <Button fx:id="addEquipmentBtn" layoutX="97.0" layoutY="316.0" mnemonicParsing="false" onAction="#addEquipment" prefHeight="26.0" prefWidth="116.0" text="Add equipment" />
      <Button fx:id="updEquipmentBtn" layoutX="97.0" layoutY="351.0" mnemonicParsing="false" onAction="#updateEquipment" text="Update equipment" />
      <Button fx:id="delEquipmentBtn" layoutX="96.0" layoutY="384.0" mnemonicParsing="false" onAction="#deleteEquipment" prefHeight="26.0" prefWidth="116.0" text="Delete equipment" />
