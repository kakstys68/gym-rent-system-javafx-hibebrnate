<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="166.0" prefWidth="182.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.LoginPage">
   <children>
      <TextField fx:id="usernameField" layoutX="14.0" layoutY="23.0" promptText="Username" />
      <PasswordField fx:id="passwordField" layoutX="15.0" layoutY="59.0" promptText="Password" />
      <Button layoutX="15.0" layoutY="121.0" mnemonicParsing="false" onAction="#validate" text="Login" />
      <Button layoutX="108.0" layoutY="121.0" mnemonicParsing="false" onAction="#openRegistration" text="Sign up" />
      <CheckBox fx:id="isRentProvider" layoutX="13.0" layoutY="91.0" mnemonicParsing="false" text="Rent Provider" />
   </children>
</AnchorPane>
